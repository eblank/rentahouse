<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tenement.mapper.UserMapper" >

  <insert id="insert" parameterType="com.tenement.model.User">
    insert into user(nick_name, real_name,create_time,login_password
      )
    values (#{nickName,jdbcType=VARCHAR}, #{realName,jdbcType=VARCHAR}, #{createTime}, #{loginPassword,jdbcType=VARCHAR}
      )
  </insert>
</mapper>